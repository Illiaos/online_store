@model Book[]

@{
    ViewData["Title"] = "Search Result";
}

<h1>Search Result</h1>

@if (Model.Length == 0)
{
    <p>Results Not Found</p>
}
else
{
    <ul>
        @foreach(var book in Model)
        {
            <li><a href="@Url.Action("Index", "Book", new {id = book.Id})">@book.Title</a></li>
        }
    </ul>
}
